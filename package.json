{
  "name": "browseping",
  "version": "1.0.0",
  "description": "A browser extension to socialize browsing experiences",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --watch --config webpack.config.js",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "build:chrome": "cross-env NODE_ENV=production TARGET_BROWSER=chrome webpack --config webpack.config.js",
    "build:firefox": "cross-env NODE_ENV=production TARGET_BROWSER=firefox webpack --config webpack.config.js",
    "start:firefox": "web-ext run --source-dir dist",
    "analyze": "NODE_ENV=production ANALYZE=true webpack --config webpack.config.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "browser extension",
    "social",
    "browsing"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "@types/node": "^25.0.8",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/webextension-polyfill": "^0.12.4",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.10",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "dotenv-webpack": "^8.1.0",
    "linkify-react": "^4.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-paginate": "^8.3.0",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.0.2",
    "webextension-polyfill": "^0.12.0"
  }
}
